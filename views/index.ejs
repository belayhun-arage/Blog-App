<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %> 
<body>
    <%- include('./partials/nav.ejs') %> 
    <div class="blogs content">
        <h2>All Blogs</h2>
        <% if(blogs.length >0) { %> 
                <% blogs.forEach(blog=>{ %> 
                     <a class="single" href="/blogs/<%=blog._id %> ">
                        <h1 class="title"><%= blog.title %> </h1>
                        <h2 class="snippet"><%= blog.snippet %> </h2>
                     </a>
                <% }) %> 
        <% } %> 
    </div>
    <%- include('./partials/footer.ejs') %>
</body>
</html>